<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.linuxtools.tmf.ui.tracetypeui">
      <type
            icon="icons/btf.png"
            tracetype="org.eclipse.linuxtools.btf.trace">
      </type>
   </extension>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="org.eclipse.tracecompass.btf.core.event.BtfEvent"
            class="org.eclipse.tracecompass.btf.ui.BtfEventAdapterFactory">
         <adapter
               type="org.eclipse.ui.views.properties.IPropertySource">
         </adapter>
      </factory>
   </extension>
   <extension
         point="org.eclipse.linuxtools.tmf.analysis.xml.core.files">
      <xmlfile
            file="btf-view.xml">
      </xmlfile>
   </extension>
</plugin>
