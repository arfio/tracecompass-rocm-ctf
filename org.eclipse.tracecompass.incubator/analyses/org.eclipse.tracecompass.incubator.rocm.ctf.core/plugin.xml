<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.linuxtools.tmf.core.tracetype">
      <type
            category="org.eclipse.linuxtools.tmf.ctf.core.category.ctf"
            event_type="org.eclipse.tracecompass.tmf.core.event.TmfEvent"
            id="org.eclipse.tracecompass.incubator.rocm.ctf.core.trace"
            isDirectory="true"
            name="%trace.rocm"
            trace_type="org.eclipse.tracecompass.incubator.rocm.ctf.core.trace.RocmCtfTrace">
      </type>
   </extension>
   <extension 
   		point="org.eclipse.linuxtools.tmf.core.analysis">
      <module
            analysis_module="org.eclipse.tracecompass.incubator.internal.rocm.ctf.core.analysis.RocmCtfCallStackAnalysis"
            automatic="true"
            id="org.eclipse.tracecompass.incubator.rocm.core.ctf.analysis.callstack"
            name="ROCm API Analysis">
         <tracetype
               applies="true"
               class="org.eclipse.tracecompass.incubator.rocm.ctf.core.trace.RocmCtfTrace">
         </tracetype>
      </module>
   </extension>
</plugin>